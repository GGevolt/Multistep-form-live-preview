{
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es2016",
    "jsx": "preserve",
    "checkJs": true,
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["./src/pages/*"],
      "@provider/*": ["./src/provider/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
